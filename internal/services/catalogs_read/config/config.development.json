{
  "appOptions": {
    "serviceName": "catalogs_read_service",
    "deliveryType": "http"
  },
  "grpcOptions": {
    "name": "catalogs_read_service",
    "port": ":6004",
    "host": "localhost",
    "development": true
  },
  "echoHttpOptions": {
    "name": "catalogs_read_service",
    "port": ":7001",
    "development": true,
    "timeout": 30,
    "basePath": "/api/v1",
    "host": "http://localhost",
    "productsPath": "products",
    "debugHeaders": true,
    "httpClientDebug": true,
    "debugErrorsResponse": true,
    "ignoreLogUrls": [
      "metrics"
    ]
  },
  "logOptions": {
    "level": "debug",
    "logType": 0,
    "callerEnabled": false
  },
  "rabbitmqOptions": {
    "rabbitmqHostOptions": {
      "userName": "guest",
      "password": "guest",
      "hostName": "localhost",
      "port": 5672
    }
  },
  "redisOptions": {
    "host": "localhost",
    "port": 6379,
    "password": "",
    "database": 0,
    "poolSize": 300
  },
  "mongoDbOptions": {
    "host": "localhost",
    "port": 27017,
    "user": "admin",
    "password": "admin",
    "database": "catalogs_service",
    "useAuth": true
  },
  "mongoCollections": {
    "products": "products",
    "categories": "categories"
  },
  "OpenTelemetryOptions": {
    "enable": true,
    "serviceName": "catalogs-read-service",
    "instrumentationName": "github.com/mehdihadeli/go-ecommerce-microservices/internal/services/catalogs/read_service",
    "id": 1,
    "alwaysOnSampler": true,
    "jaegerExporterConfig": {
      "agentHost": "localhost",
      "agentPort": "6831"
    },
    "zipkinExporterConfig": {
      "url": "http://localhost:9411/api/v2/spans"
    },
    "otelMetricsOptions": {
      "port": ":2221",
      "metricsRoutePath": "/metrics",
      "name": "catalogs-read-service"
    }
  },
  "elastic": {
    "url": "http://localhost:9200",
    "sniff": false,
    "gzip": true,
    "explain": true,
    "fetchSource": true,
    "version": true,
    "pretty": true
  },
  "elasticIndexes": {
    "products": "products"
  },
  "eventStoreConfig": {
    "connectionString": "esdb://localhost:2113?tls=false"
  }
}
